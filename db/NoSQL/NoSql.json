{
  "chat": {
    "createdAt": "(datetime)",
    "id": "(var char)",
    "userA": {
      "fullname": "(string)",
      "age": "(int)",
      "id": "(var char)"
    },
    "userB": {
      "fullname": "(string)",
      "age": "(int)",
      "id": "(var char)"
    },
    "messages": [
      {
        "sourceUser": "(int)",
        "media(optional)": "(url)",
        "text(optional)": "(string)",
        "createdAt": "(datetime)"
      }
    ]
  },
  "profileViews": {
    "id": "(var char)",
    "viewedUser": "(var char) ID",
    "viewers": [
      {
        "createdAt": "(datetime)",
        "user": "(var char) ID"
      }
    ]
  },
  "notifications": {
    "id": "(var char)",
    "userID": "(var char)",
    "type": "ENUM",
    "imageURL": "(url)",
    "title": "(string)",
    "body": "(string)",
    "createdAt": "(datetime)"
  },
  "userData": {
    "id": "(var char)",
    "gender": "ENUM(Male/Female)",
    "bio": "(string)",
    "birthDate": "(datetime)",
    "age": "(int)",
    "location": {
      "lng": "(double)",
      "lat": "(double)",
      "addressName": "string"
    },
    "links": {
      "facebook": "(url)",
      "instagram": "(url)",
      "spotify": "(url)"
    },
    "profilePictures": [
      {
        "url": "(string)",
        "isMain": "(boolean)",
        "uploadedAt": "(datetime)"
      }
    ],
    "videos": ["(url)"],
    "interests": [
      {
        "interestID": "(var char)",
        "name": "(string)"
      }
    ],
    "preferences": {
      "age": "(int)",
      "location(radius)": "(int)"
    }
  }
}
